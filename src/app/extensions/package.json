{
  "name": "ui-extensions-react-examples",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "main": "Example.jsx",
  "scripts": {
    "bump": "npm install -s @hubspot/ui-extensions@latest && npm install -D @hubspot/ui-extensions-dev-server@latest",
    "setup": "npm run bump && npm ci",
    "clean-install-local": "rm -rf node_modules/ package-lock.json && npm update && npm install --install-links",
    "developing-local-dev-server": "../../../scripts/useLocalVersionOfDevServer.js && npm run clean-install-local",
    "developing-local-components": "../../../scripts/useLocalVersionOfComponents.js && npm run clean-install-local",
    "dev": "hs-ui-extensions-dev-server dev",
    "build": "hs-ui-extensions-dev-server build",
    "integration-test": "../../../../../ui-extensibility/public-packages/ui-extensions-dev-server/integrationTests/runTests.js",
    "local-dev": "npm run developing-local-dev-server && hs-ui-extensions-dev-server dev",
    "local-build": "npm run developing-local-dev-server && hs-ui-extensions-dev-server build",
    "local-integration-test": "DEBUG='true' npm run developing-local-dev-server && ../../../../../ui-extensibility/public-packages/ui-extensions-dev-server/integrationTests/runTests.js",
    "latest": "../../../scripts/switchBackToLatest.js && npm run clean-install-local",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HubSpot/ui-extensions-react-examples"
  },
  "dependencies": {
    "@hubspot/ui-extensions": "^0.7.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@hubspot/ui-extensions-dev-server": "../../../../../ui-extensibility/public-packages/ui-extensions-dev-server/"
  }
}